{
  "version": 2,
  "builds": [
    {
      "src": "app_minimal.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "memory": 512
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app_minimal.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "FLASK_ENV": "production"
  }
}
